{% extends 'base.html.twig' %}
{% block body %}
<div class="restricted_container">
  <h1>Nos sponsors & partenaires</h1>
  <div class="galery-container">
    {% for sponsor in sponsors %}
      <div class="box">
        <img src="{{asset('sponsor_images/' ~ sponsor.imageName)}}"
        alt="{{sponsor.title}}" class="image">
      </div>
    {% endfor %}
  </div>
  <!-- The expanding image container -->
  <div class="container">
    <!-- Close the image -->
    <span  class="closebtn">&times;</span>

    <!-- Expanded image -->
    <img id="expandedImg">
  </div>
</div>
{% endblock %}
